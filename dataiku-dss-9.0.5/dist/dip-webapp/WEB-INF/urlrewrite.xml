<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule><from>^/dip/api/(.*)</from><to>/api/$1</to></rule>
    <rule><from>^/shakers/(.*)</from><to>/index.html</to></rule>
    <rule><from>^/datasets/(.*)</from><to>/index.html</to></rule>
    <rule><from>^/feedback/(.*)</from><to>/index.html</to></rule>
</urlrewrite>