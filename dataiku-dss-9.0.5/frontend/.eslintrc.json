{
    "env": {
        "browser": true,
        "es2017": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "ignorePatterns": [
        "**/js/flow/filter/compiled-js/*",
        "**/webpack.config.js",
        "**/flow-filter-bundle.js",
        "**/flow-styling.js"
    ],
    "rules": {
        "no-undef": "error",
        "no-redeclare": "error",
        "no-empty": "error",
        "no-self-assign": "error",
        "constructor-super": "warn",
        "for-direction": "warn",
        "getter-return": "warn",
        "no-async-promise-executor": "warn",
        "no-case-declarations": "warn",
        "no-class-assign": "warn",
        "no-compare-neg-zero": "warn",
        "no-cond-assign": "warn",
        "no-const-assign": "warn",
        "no-constant-condition": "warn",
        "no-control-regex": "warn",
        "no-debugger": "warn",
        "no-delete-var": "warn",
        "no-dupe-args": "warn",
        "no-dupe-class-members": "warn",
        "no-dupe-else-if": "warn",
        "no-dupe-keys": "warn",
        "no-duplicate-case": "warn",
        "no-empty-character-class": "warn",
        "no-empty-pattern": "warn",
        "no-ex-assign": "warn",
        "no-extra-boolean-cast": "warn",
        "no-fallthrough": "warn",
        "no-func-assign": "warn",
        "no-global-assign": "warn",
        "no-import-assign": "warn",
        "no-inner-declarations": "warn",
        "no-invalid-regexp": "warn",
        "no-irregular-whitespace": "warn",
        "no-misleading-character-class": "warn",
        "no-new-symbol": "warn",
        "no-obj-calls": "warn",
        "no-octal": "warn",
        "no-prototype-builtins": "warn",
        "no-regex-spaces": "warn",
        "no-setter-return": "warn",
        "no-shadow-restricted-names": "warn",
        "no-sparse-arrays": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "no-unsafe-finally": "warn",
        "no-unsafe-negation": "warn",
        "no-unused-labels": "warn",
        "no-unused-vars": "warn",
        "no-useless-catch": "warn",
        "no-useless-escape": "warn",
        "no-with": "warn",
        "require-yield": "warn",
        "use-isnan": "warn",
        "valid-typeof": "warn"
    },
    "globals": { // thirdy party libs + lot of stuff from utils.js, logging.js, fragments => see ch62843
        "Mousetrap": false,
        "d3": false,
        "angular": false,
        "jQuery": false,
        "$": false,
        "L": false, // Leaflet
        "_": false,
        "CodeMirror": false,
        "Intercom": false,
        "Alation": false,
        "Handsontable": false,
        "Messenger": false,
        "printStackTrace": false,
        "canvg": false,
        "fattable": false,
        "moment": false,
        "marked": false,
        "Spinner": false,
        "ChartIAE": false,
        "sanitize": false,
        "graphVizUnescape": false,
        "html2canvas_latest": false,
        "FlowFilterParser": false,
        "addDatasetUniquenessCheck": false,
        "computeCodeMirrorMode": false,
        "getDefaultConnection": false,
        "fetchManagedDatasetConnections": false,
        "filterSortLimitTagsAndQ": false,
        "setErrorInScope": false,
        "setErrorInScope2": false,
        "getErrorDetails": false,
        "getErrorHTMLFromDetails": false,
        "sqlFormatter": false,
        "resetErrorInScope": false,
        "isTouchDevice": false,
        "resolveObjectSmartId": false,
        "resolveDatasetFullName": false,
        "dkuDeepCopy": false,
        "checkChangesBeforeLeaving": false,
        "getCookie": false,
        "downloadURL": false,
        "makeSVG": false,
        "graphVizEscape": false,
        "makeFullModelEvalutionIdString": false,
        "isInteger": false,
        "fakeClickOnLink": false,
        "cleanupVariable": false,
        "safeApply": false,
        "resolveValue": false,
        "getRewrappedPromise": false,
        "rextend": false,
        "treeToList": false,
        "convertDateFromTimezone": false,
        "convertDateToTimezone": false,
        "regexLastIndexOf": false,
        "getDayLabels": false,
        "urlWithProtocolAndHost": false,
        "algoliasearch": false,
        "algoliasearchHelper": false,
        "alphabeticalSort": false,
        "ensureVisible": false,
        "dkuEvents": false,
        "getDigestTime": false,
        "durationHHMM": false,
        "durationHHMMSS": false,
        "log10": false,
        "formatDateToISOLocalDate": false,
        "formatDateToISOLocalDateTime": false,
        "arr2obj": false,
        "smartLogTailToHTML": false,
        "generateRandomId": false,
        "clearSelection": false,
        "obj2arr": false,
        "getTextWidth": false,
        "removeFirstFromArray": false,
        "clear": false,
        "getPlacementForMouse": false,
        "getSelectionInElement": false,
        "getPlacement2": false,
        "listDifference": false,
        "moveItemInArray": false,
        "listCopyContent": false,
        "zip": false,
        "arrayDedup": false,
        "generateUniqueId": false,
        "searchTree": false,
        "graphIdFor": false,
        "makeFullModelIdStringFromObject": true,
        "makeFullModelIdStringFromEvaluation": true,
        "makeModelLikeIDStringFromObject": true
    }
}
